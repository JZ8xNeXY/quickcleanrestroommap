{
  "family": "quickcleanrestroom-task-definition-frontend",
  "containerDefinitions": [
      {
          "name": "next",
          "image": "606780327190.dkr.ecr.ap-northeast-1.amazonaws.com/quickcleanrestroommap-next",
          "cpu": 0,
          "portMappings": [
              {
                  "name": "next-80-tcp",
                  "containerPort": 80,
                  "hostPort": 80,
                  "protocol": "tcp",
                  "appProtocol": "http"
              }
          ],
          "essential": true,
          "environment": [
            // {
            //     "name": "NEXT_PUBLIC_API_BASE_URL",
            //     "value": "${NEXT_PUBLIC_API_BASE_URL}"
            // },
            // {
            //     "name": "NEXT_PUBLIC_GOOGLE_MAP_KEY",
            //     "value": "${NEXT_PUBLIC_GOOGLE_MAP_KEY}"
            // },
            // {
            //     "name": "NEXT_PUBLIC_OPENAI_API_KEY",
            //     "value": "${NEXT_PUBLIC_OPENAI_API_KEY}"
            // }
        ],
          "environmentFiles": [],
          "mountPoints": [],
          "volumesFrom": [],
          "ulimits": [],
          "logConfiguration": {
              "logDriver": "awslogs",
              "options": {
                  "awslogs-group": "/ecs/quickcleanrestroom-task-definition-frontend",
                  "awslogs-create-group": "true",
                  "awslogs-region": "ap-northeast-1",
                  "awslogs-stream-prefix": "ecs"
              },
              "secretOptions": []
          },
          "systemControls": []
      }
  ],
  "taskRoleArn": "arn:aws:iam::606780327190:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::606780327190:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
      "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
      "cpuArchitecture": "X86_64",
      "operatingSystemFamily": "LINUX"
  }
}